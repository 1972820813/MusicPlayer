<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.keeasy.musicplayer">

    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".other.ui.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".other.ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".other.ui.SettingsActivity"/>
        <activity android:name=".other.ui.SearchActivity"/>
        <activity android:name=".other.activity.MemberCenterActivity"/>
        <activity android:name=".other.activity.ShoppingActivity"/>
        <activity android:name=".other.activity.OnLineActivity"/>
        <activity android:name=".other.activity.MyFriendActivity"/>
    <activity android:name=".other.activity.AlarmActivity"/>

    </application>

</manifest>